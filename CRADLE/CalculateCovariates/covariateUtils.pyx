		elif newDimer[1] == 67:
			dimerIdx += 1
		elif newDimer[1] == 71:
			dimerIdx += 2
		elif newDimer[1] == 84:
			dimerIdx += 3
		gibbs = gibbs + GIBBS[dimerIdx][1]

	## remove the old dimer for the next iteration
	if b'N' in oldDimer:
		subtract = n_gibbs
	else:
		dimerIdx = 0
		if oldDimer[0] == 65:
			pass  # dimerIdx = dimerIdx
		elif oldDimer[0] == 67:
			dimerIdx += 4
		elif oldDimer[0] == 71:
