@lru_cache(maxsize=4_096)
def editStartGibbs(oldDimer, newDimer, double pastStartGibbs, double n_gibbs):
	cdef double gibbs = pastStartGibbs
	cdef double subtract = -1
	cdef int dimerIdx = 0

	# newDimer
	if b'N' in newDimer:
		gibbs += n_gibbs
	else:
		dimerIdx = 0
		if newDimer[0] == 65:
			pass  # dimerIdx = dimerIdx
		elif newDimer[0] == 67:
			dimerIdx += 4
		elif newDimer[0] == 71:
			dimerIdx += 8
		elif newDimer[0] == 84:
			dimerIdx += 12

