@lru_cache(maxsize=1_024)
def convertGibbs(double gibbs, double entropy, double fragLen, double min_tm, double max_tm, double para1, double para2):
	cdef double anneal, denature

	tm = gibbs / (entropy*(fragLen-1))
	tm = (tm - min_tm) / (max_tm - min_tm)

	## anneal
	anneal = ( math.exp(tm) - para1 ) * para2
	anneal = np.log(anneal)

	## denature
	tm -= 1
	denature = ( math.exp( tm*(-1) ) - para1 ) * para2
	denature = np.log(denature)

	return anneal, denature
